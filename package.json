{
  "name": "nextjs-react-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for Next.js + React app with dev and Now deploy config.",
  "scripts": {
    "build": "next build",
    "start": "npm run server:dev",
    "server:dev": "next",
    "server:build": "next start",
    "lint-eslint": "eslint components pages --ext .js,.jsx",
    "lint": "npm run lint-eslint",
    "deploy": "now --public && now alias && now rm $npm_package_now_name --safe --yes",
    "jest": "jest --watch",
    "test": "jest --coverage"
  },
  "now": {
    "name": "nextjs-react-boilerplate"
  },
  "dependencies": {
    "next": "^7.0.2",
    "prop-types": "^15.7.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moreawarefly/nextjs-react-boilerplate"
  },
  "keywords": [
    "nextjs",
    "react",
    "boilerplate",
    "now",
    "eslint",
    "editorconfig"
  ],
  "author": "Marcin Skawi≈Ñski",
  "license": "MIT"
}
